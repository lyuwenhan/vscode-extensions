{
	"name": "code-formatter-and-minifier",
	"displayName": "Code Formatter & Minifier",
	"private": false,
	"version": "1.0.12",
	"publisher": "lyuwenhan",
	"description": "A VS Code extension to minify and beautify JavaScript, and sort JSON files with one click.",
	"engines": {
		"vscode": "^1.80.0"
	},
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onLanguage:jsonl"
	],
	"icon": "media/icon.png",
	"scripts": {
		"vscode:prepublish": "node build.js",
		"package": "vsce package --allow-missing-repository"
	},
	"dependencies": {
		"js-beautify": "^1.15.4",
		"jsonc-parser": "^3.3.1",
		"jsonparse": "^1.3.1",
		"terser": "^5.44.0"
	},
	"devDependencies": {
		"@vscode/vsce": "^3.0.0",
		"esbuild": "^0.25.10"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lyuwenhan/vscode-extensions",
		"directory": "code-formatter-and-minifier"
	},
	"bugs": {
		"url": "https://github.com/lyuwenhan/vscode-extensions/issues"
	},
	"homepage": "https://lyuwenhan.github.io",
	"categories": [
		"Formatters"
	],
	"keywords": [
		"formatter",
		"minify",
		"beautify",
		"json",
		"jsonc",
		"jsonl",
		"uuid"
	],
	"contributes": {
		"commands": [
			{
				"command": "minifier.generateUuid",
				"title": "Generate UUID",
				"category": "Minifier"
			},
			{
				"command": "minifier.minifySel",
				"title": "Minify current selection",
				"category": "Minifier"
			},
			{
				"command": "minifier.beautifySel",
				"title": "Beautify current selection",
				"category": "Minifier"
			},
			{
				"command": "minifier.mitifySel",
				"title": "Mitify current selection",
				"category": "Minifier"
			},
			{
				"command": "minifier.sortSel",
				"title": "Sort current selection",
				"category": "Minifier"
			},
			{
				"command": "minifier.minify",
				"title": "Minify current file",
				"category": "Minifier"
			},
			{
				"command": "minifier.beautify",
				"title": "Beautify current file",
				"category": "Minifier"
			},
			{
				"command": "minifier.mitify",
				"title": "Mitify current file",
				"category": "Minifier"
			},
			{
				"command": "minifier.sort",
				"title": "Sort current file",
				"category": "Minifier"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "minifier.generateUuid",
					"group": "navigation@91",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled')"
				},
				{
					"command": "minifier.minifySel",
					"group": "navigation@92",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && editorHasSelection && (resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl)"
				},
				{
					"command": "minifier.beautifySel",
					"group": "navigation@93",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && editorHasSelection && (resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl)"
				},
				{
					"command": "minifier.mitifySel",
					"group": "navigation@94",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && editorHasSelection && (resourceLangId == javascript)"
				},
				{
					"command": "minifier.sortSel",
					"group": "navigation@95",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && editorHasSelection && (resourceLangId == json || resourceLangId == jsonc)"
				},
				{
					"command": "minifier.minify",
					"group": "navigation@96",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && !editorHasSelection && (resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl)"
				},
				{
					"command": "minifier.beautify",
					"group": "navigation@97",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && !editorHasSelection && (resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl)"
				},
				{
					"command": "minifier.mitify",
					"group": "navigation@98",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && !editorHasSelection && (resourceLangId == javascript)"
				},
				{
					"command": "minifier.sort",
					"group": "navigation@99",
					"when": "editorTextFocus && (resourceScheme == 'file' || resourceScheme == 'untitled') && !editorHasSelection && (resourceLangId == json || resourceLangId == jsonc)"
				}
			],
			"commandPalette": [
				{
					"command": "minifier.generateUuid",
					"when": "false"
				},
				{
					"command": "minifier.minifySel",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.beautifySel",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.mitifySel",
					"when": "resourceLangId == javascript"
				},
				{
					"command": "minifier.sortSel",
					"when": "resourceLangId == json || resourceLangId == jsonc"
				},
				{
					"command": "minifier.minify",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.beautify",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.mitify",
					"when": "resourceLangId == javascript"
				},
				{
					"command": "minifier.sort",
					"when": "resourceLangId == json || resourceLangId == jsonc"
				}
			],
			"explorer/context": [
				{
					"command": "minifier.minify",
					"group": "navigation@96",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.beautify",
					"group": "navigation@97",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.mitify",
					"group": "navigation@98",
					"when": "resourceLangId == javascript"
				},
				{
					"command": "minifier.sort",
					"group": "navigation@99",
					"when": "resourceLangId == json || resourceLangId == jsonc"
				}
			],
			"editor/title/context": [
				{
					"command": "minifier.minify",
					"group": "navigation@96",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.beautify",
					"group": "navigation@97",
					"when": "resourceLangId == javascript || resourceLangId == json || resourceLangId == jsonc || resourceLangId == jsonl"
				},
				{
					"command": "minifier.mitify",
					"group": "navigation@98",
					"when": "resourceLangId == javascript"
				},
				{
					"command": "minifier.sort",
					"group": "navigation@99",
					"when": "resourceLangId == json || resourceLangId == jsonc"
				}
			]
		}
	}
}