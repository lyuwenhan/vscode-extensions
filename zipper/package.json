{
  "name": "zipper",
  "displayName": "Zipper",
  "private": false,
  "version": "1.0.0",
  "publisher": "lyuwenhan",
  "description": "A VS Code extension to do compression.",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "icon": "media/icon.png",
  "scripts": {
    "vscode:prepublish": "node build.js",
    "package": "vsce package --allow-missing-repository"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyuwenhan/vscode-extensions",
    "directory": "zipper"
  },
  "bugs": {
    "url": "https://github.com/lyuwenhan/vscode-extensions/issues"
  },
  "dependencies": {
    "archiver": "^7.0.1"
  },
  "homepage": "https://lyuwenhan.github.io",
  "categories": [
    "Other"
  ],
  "keywords": [],
  "contributes": {
    "commands": [
      {
        "category": "Zipper",
        "command": "zipper.compress",
        "title": "Compress"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "zipper.compress",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "group": "navigation@80",
          "command": "zipper.compress",
          "when": "true"
        }
      ]
    }
  }
}